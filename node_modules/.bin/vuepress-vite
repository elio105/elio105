#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_ca74985b397bf90ecda6674ec52f3653/node_modules/vuepress/bin/node_modules:/mnt/d/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_ca74985b397bf90ecda6674ec52f3653/node_modules/vuepress/node_modules:/mnt/d/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_ca74985b397bf90ecda6674ec52f3653/node_modules:/mnt/d/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_ca74985b397bf90ecda6674ec52f3653/node_modules/vuepress/bin/node_modules:/mnt/d/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_ca74985b397bf90ecda6674ec52f3653/node_modules/vuepress/node_modules:/mnt/d/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_ca74985b397bf90ecda6674ec52f3653/node_modules:/mnt/d/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vuepress@2.0.0-rc.20_@vuepr_ca74985b397bf90ecda6674ec52f3653/node_modules/vuepress/bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vuepress@2.0.0-rc.20_@vuepr_ca74985b397bf90ecda6674ec52f3653/node_modules/vuepress/bin/vuepress-vite.js" "$@"
fi
